<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<!-- Import customized bean configurations for chosen project. -->
	<import resource="services-config_VGR.xml"/>
<!--	<import resource="services-config_LTH.xml"/>-->

<!-- Search service mock implementation 
 	<bean id="Search_SearchService" class="se.vgregion.kivtools.search.svc.impl.mock.SearchServiceMockImpl" />
-->  	

	<bean name="KivTools_CreateLinkLists" class="org.springframework.scheduling.quartz.JobDetailBean">
	  <property name="jobClass" value="se.vgregion.kivtools.search.scheduler.LinkListGenerator" />
	  <property name="jobDataAsMap">
	  	<map>
	  		<entry key="searchService" value-ref="Search_SearchService" />
	  		<entry key="file" value="units.html" />
	  	</map>
	  </property>
	</bean>
</beans>
